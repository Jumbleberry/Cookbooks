{
    "databases": {
        "titan"    : {
            "adapter"     : "Mysql",
            "host"        : "<%= @mysql_write_host %>",
            "username"    : "<%= @mysql_write_username %>",
            "password"    : "<%= @mysql_write_password %>",
            "dbname"      : "<%= @mysql_write_database %>",
            "persistent"  : true
        }
    },
    "oauth": {
        "url" : "<%= node['oauth']['url'] %>",
        "callback" : "<%= node['oauth']['callback'] %>",
        "key" : "<%= node['oauth']['key'] %>",
        "secret" : "<%= node['oauth']['secret'] %>"
    }
}
