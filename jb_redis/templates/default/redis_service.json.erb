{
    "service": {
        "id": "<%= @name || 'redis' %><%= @port %>",
        "name": "<%= @name || 'redis' %>",
        "port": <%= @port %>,
        "address": "<%= @address || '' %>",
        "tags": [
            "<%= @name || 'redis' %>",
            "<%= @tag %>"
        ],
        "check": {
            "script": "<%= @consul_path %>/redis_check.php <%= @currentip %> <%= @port %>",
            "interval": "5s"
        },
        "enableTagOverride": true
    }
}