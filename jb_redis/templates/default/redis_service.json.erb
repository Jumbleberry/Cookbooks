{
    "service": {
        "id": "redis<%= @port %>",
        "name": "redis",
        "port": <%= @port %>,
        "address": "<%= @currentip || '' %>",
        "tags": [
            "redis",
            "<%= @tag %>"
        ],
        "check": {
            "script": "<%= @consul_path %>/redis_check.php <%= @currentip %> <%= @port %>",
            "interval": "5s"
        },
        "enableTagOverride": true
    }
}