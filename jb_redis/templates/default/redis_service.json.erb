{
    "service": {
        "id": "<%= @name || 'redis' %><%= @port %>",
        "name": "<%= @name || 'redis' %>",
        "port": <%= @port %>,
        "address": "<%= @address || '' %>",
        "tags": [
            "<%= @name || 'redis' %>",
            "<%= @tag %>"
        ],
        "check": {
            "args": ["<%= @consul_path %>/redis_check.php", "<%= @currentip %>", "<%= @port %>"],
            "interval": "5s"
        },
        "enable_tag_override": true
    }
}
